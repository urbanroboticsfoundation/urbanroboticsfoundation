RequestTripTicket DEFINITIONS AUTOMATIC TAGS ::= 
BEGIN
  TripRequest ::= SEQUENCE       
  {
     startLocation UTF8String (SIZE(1..16)),
     endLocation UTF8String (SIZE(1..16)),
     arrivalTime DATE-TIME,
     vehicleGrossWeight REAL,
     vehicleHeight REAL,
     vehicleLength REAL,
     vehicleWidth REAL
  }                                                     
END
